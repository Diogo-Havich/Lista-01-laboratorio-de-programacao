#include <stdio.h>
int main (){
  int dias;
  float valor = 50.25, bruto, liquido;
  printf ("Digite os dias trabalhados\n");
  scanf ("%d", &dias);
  if (dias <= 10 ){
    bruto = valor * dias;
    liquido = ((valor * dias) * (-0.1) + bruto);
    }
   else if (dias > 10 && dias <= 20){
    bruto = valor * dias * 1.2;
    liquido = ((valor * dias * 1.2) * (-0.1) + bruto);
     }
   else{
    bruto = valor * dias * 1.3;
    liquido = ((valor * dias * 1.3) * (-0.1) + bruto);
     }
  printf("Valor lÃ­quido = %.2f\n",liquido);
return 0;}
