#include <stdio.h>
#include <math.h>

int main()
  {
  float x1, x2, y1, y2, d;
  puts("Digite os valores de x1 e y1");
  scanf("%f%f", &x1, &y1);
  puts("Digite os valores de x2 e y2");
  scanf("%f%f", &x2, &y2);
  d = sqrt(((x2 - x1) * (x2 - x1)) + ((y2 - y1) * (y2 - y1)));
  printf("%.2f\n", d);
  return 0;
  }
