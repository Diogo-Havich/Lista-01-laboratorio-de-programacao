#include <stdio.h>
int main (){
  float valor, dias, bruto, liquido, horas, percentual;
  printf ("Digite o valor da hora trabalhada\n");
  scanf ("%f", &valor);
  printf ("Digite as horas trabalhadas por dia\n");
  scanf ("%f", &horas);
  printf ("Digite os dias trabalhados no mes\n");
  scanf ("%f", &dias);
  printf ("Digite o percentual do INSS\n");
  scanf ("%f", &percentual);
    bruto = valor * dias * horas;
    percentual = percentual / 100;  
    liquido = (bruto * (-percentual)) + bruto;
  printf("Valor bruto = %.2f Valor l√≠quido = %.2f\n", bruto, liquido);
return 0;}
